{
    "include": [
        "dayflow",
        "tests"
    ],
    "exclude": [
        "**/__pycache__",
        "**/node_modules",
        "**/.venv",
        "**/venv",
        "build",
        "dist"
    ],
    "defineConstant": {
        "DEBUG": true
    },
    "venvPath": ".",
    "venv": ".venv",
    "reportMissingImports": true,
    "reportMissingTypeStubs": false,
    "pythonVersion": "3.8",
    "pythonPlatform": "All",
    "typeCheckingMode": "basic",
    "useLibraryCodeForTypes": true
}
